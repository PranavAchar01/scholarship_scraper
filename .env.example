# API Keys for scholarship data sources
COLLEGE_SCORECARD_API_KEY=your_college_scorecard_api_key_here
FEDERAL_AID_API_KEY=your_federal_aid_api_key_here
CAREERONESTOP_API_KEY=your_careeronestop_api_key_here
FASTWEB_API_KEY=your_fastweb_api_key_here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/scholarship_scraper
# For development with SQLite
# DATABASE_URL=file:./dev.db

# Ollama Configuration
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama3.1

# Redis for caching (optional)
REDIS_URL=redis://localhost:6379

# Rate limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Analytics (optional)
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX